{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": "demo\\.java",
      "matchStrings": [
        "//DEPS (?<depName>.+?):(?<currentValue>.+)"
      ],
      "datasourceTemplate": "maven"
    }
  ]
}
